void setup() 
{
  Serial.begin(9600);
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }
  display.clearDisplay();
  display.display();
  delay(2000);
  pinMode(button_pin, INPUT_PULLUP);
  pinMode(buzz_pin, OUTPUT);
}

void single() {
  display.clearDisplay();
  display.drawBitmap(0, 0, single_jump, 128, 64, WHITE);
  display.display();
}
void As_much_data_as_possible() {
  display.clearDisplay();
  display.drawBitmap(0, 0, AMDAP, 128, 64, WHITE);
  display.display();
}
void buzz() {
  display.clearDisplay();
  display.drawBitmap(0, 0, BUZZER, 128, 64, WHITE);
  display.display();
}
void jheight() {
  display.clearDisplay();
  display.drawBitmap(0, 0, JUMP_HEIGHT, 128, 64, WHITE);
  display.display();
}
void noise() {
  if(go_buzz == true){
    digitalWrite(buzz_pin, HIGH);
    delay(200);
    digitalWrite(buzz_pin, LOW);
  }
}
void menu() { //Put all of the menu into a sepperate function so that I can call it when the button is pressed
  int i = 0;
  delay(300);
  while(i < 10) {
    value = analogRead(pot_pin); // Redefines the values of the things
    pressed = digitalRead(button_pin);
    display.display();
    if(value < 255) {// Calls each of the different pictures when it is selected
      jheight();
      chosen = 0;
    } else if((value > 255) && (value < 610)){
      single();
      chosen = 1;
    } else if((value > 610) && (value < 865)){
      As_much_data_as_possible();
      chosen = 2;
    } else if (value > 865) {
      buzz();
      chosen = 3;
    }
    if (pressed == LOW) { // IF the button is pressed than it selects that option in the muensu
      i = 20;
      noise();
      display.setTextColor(WHITE);
      display.setTextSize(2);
      display.setCursor(0,0);
      display.clearDisplay();
      if(chosen == 0) {
          display.println("JumpHeight");
      } else if(chosen == 1){
          display.println("Single Rep");
      } else if(chosen == 2){
          display.println("AMDAP");
      } else if (chosen == 3) {
          display.println("Buzzer");
          if(go_buzz == true) {
            go_buzz = false;
            display.println("OFF");
          } else {
            go_buzz = true;
            display.println("ON");
        }

    }
      display.println("SELECTED");
      display.display();
      delay(1000);
      i = 0;
      display.clearDisplay();
      display.display();
      break;
    }
  }
}
void loop() 
{
  value = analogRead(pot_pin);
  pressed = digitalRead(button_pin);
  
  if (pressed == LOW){
    menu();
  }
 
}
